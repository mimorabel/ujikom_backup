datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model motor {
  id      Int    @id @default(autoincrement())
  merk    String
  jenis   String
  tipe    String
  seri    String
  nopol   String @unique
  status  String
}

